[global_config]
  suppress_multiple_term_dialog = True
[keybindings]
  new_tab = F12
  close_term = F11
  close_window = <Primary><Shift>q
  next_tab = <Primary><Alt>n
  prev_tab = <Primary><Alt>b
  full_screen = F10
[profiles]
  [[default]]
    allow_bold = False
    cursor_color = "#aaaaaa"
    font = Monaco Bold 13
    foreground_color = "#00ff00"
    show_titlebar = False
    scrollbar_position = hidden
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
